model = AR {
	out_grid = (200,40,40)
	acf = {
		func = standing_wave
		grid = (10,10,10) : (2.5,5,5)
	}
	least_squares = 0
	order = (7,7,7)
	output = surface,acf,waves,csv
}
velocity_potential_solver = linear {
	wnmax = from (0,0) to (0,0.25) npoints (2,2)
	depth = 12
	domain = from (10,-12) to (10,4) npoints (1,128)
}
